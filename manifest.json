{
  "vendor": "tiendafc",
  "name": "product-upon-request",
  "version": "0.0.1",
  "title": "Product Upon Request",
  "description": "It is an application that allows you to select and add to the cart products that do not have stock showing a message that they will have a delay of a certain number of days.",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "store": "0.x",
    "node": "6.x",
    "graphql": "1.x"
  },
  "dependencies": {
    "vtex.product-context": "0.x"
  },
  "registries": [
    "smartcheckout"
  ],
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "*.vtexcommercestable.com.br",
        "path": "/catalog_system/pub/products/variations/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "*.vtexcommercestable.com.br",
        "path": "/logistics/pvt/inventory/skus/*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "POWER_USER_DS"
    }
  ],
  "settingsSchema": {
    "title": "Product Upon Request",
    "type": "object",
    "properties": {
      "warehouse_1": {
        "title": "Warehouse Principal",
        "type": "string"
      },
      "warehouse_2": {
        "title": "Warehouse Infinito",
        "type": "string"
      },
      "brands": {
        "description": "Separadas por comas (,)",
        "title": "Marcas afectadas",
        "type": "string"
      },
      "categories": {
        "description": "Separadas por comas (,)",
        "title": "Categor√≠as afectadas (ids)",
        "type": "string"
      }
    }
  },
  "billingOptions": {
    "termsURL": "",
    "support": {
      "url": ""
    },
    "free": true
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
